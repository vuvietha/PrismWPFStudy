<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CommonModule.Views"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars" x:Class="CommonModule.Views.Toolbar"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="800"
            xmlns:mvvm="http://prismlibrary.com/"
            mvvm:ViewModelLocator.AutoWireViewModel="True">


    <DockPanel>
        <dx:SimpleButton Command="{Binding ApplicationCommands.CompositeCommand}" Content="Get All" Height="30" VerticalAlignment="Top" Margin="0" Width="100"/>
        <dx:SimpleButton Content="Save All" Height="30" VerticalAlignment="Top" Margin="20,0,0,0" Width="100"/>
        <dx:SimpleButton Command="{Binding ApplicationCommands.DeleteCommand}" Content="Delete All" Height="30" VerticalAlignment="Top" Margin="-400,0,0,0" Width="100"/>
    </DockPanel>


</UserControl>
